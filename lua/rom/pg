--codes--
code1 = "HYgrcRDn"
code2 = "V8vkZL1b"
code3 = "fPSLGGqC"
code4 = "HadPf3hf"
code5 = "HbX0rubx"
code6 = "jdZF2DHu"
code7 = "fHZAX4WD"
code8 = "BKrFLBA7"
code9 = "fRNavnGe"
code10 = "Jvdtz2nZ"
code11 = "UU4QeSXw"
--main--
textutils.slowPrint("Installing Gay's tow OS ver.alpha...")
shell.run("mkdir Gaystow")
shell.run("Gaystow/loginimages")
shell.run("mkdir Gaystow/storage")
shell.run("mkdir Gaystow/settings")
shell.run("mkdir Gaystow/storage/C")
shell.run("mkdir Gaystow/storage/C/terminals")
shell.run("mkdir Gaystow/storage/C/programs")
shell.run("mkdir Gaystow/storage/C/Windows")
shell.run("pastebin get " .. code1 .. " startup")
shell.run("pastebin get " .. code2 .. " desktop.jpg")
shell.run("pastebin get " .. code3 .. " desktop_overlay.jpg")
shell.run("pastebin get " .. code4 .. " loading.jpg")
shell.run("pastebin get " .. code5 .. " .login2")
shell.run("pastebin get " .. code6 .. " loginoptions")
shell.run("pastebin get " .. code7 .. " logoff")
shell.run("pastebin get " .. code8 .. " roboco.exe")
shell.run("pastebin get " .. code9 .. " splash.jpg")
shell.run("pastebin get " .. code10 .. " terminal")
shell.run("pastebin get " .. code11.. " win10.exe")
shell.run("copy desktop.jpg Gaystow/storage/C/Windows/desktop.jpg")
shell.run("delete desktop.jpg")
shell.run("copy desktop_overlay.jpg Gaystow/storage/C/Windows/desktop_overlay.jpg")
shell.run("delete desktop_overlay.jpg")
shell.run("copy loading.jpg Gaystow/storage/C/Windows/loading.jpg")
shell.run("delete loading.jpg")
shell.run("copy .login2 Gaystow/storage/C/programs/.login2")
shell.run("delete .login2")
shell.run("copy loginoptions Gaystow/settings/loginoptions")
shell.run("delete loginoptions")
shell.run("copy roboco.exe Gaystow/storage/C/terminals/roboco.exe")
shell.run("delete roboco.exe")
shell.run("copy splash.jpg Gaystow/loginimages/splash.jpg")
shell.run("delete splash.jpg")
shell.run("copy terminal Gaystow/storage/C/terminals/terminal")
shell.run("delete terminal")
shell.run("copy win10.exe Gaystow/storage/C/Windows/win10.exe")
shell.run("delete win10.exe")
--after making dirs and downloading required programs and system stuff--
print("Gaystow successfully installed onto your machine/computer/server!")
sleep(1)
--textutils.slowPrint("Starting setup...")
--shell.run("Gaystow/setup.exe")
textutils.slowPrint("Rebooting...")
sleep(.2)
os.reboot()
